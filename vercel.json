{
    "version": 2,
    "functions": {
        "server.js": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}